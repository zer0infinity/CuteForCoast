{
	/Modules {
		CacheHandlerModule
		MappersModule
		DataAccessImplsModule
	}

	/Mappers {}
	/DataAccessImpls {}

	/ConnectionTest
	{
		/WrongServer {
			/LDAPServer		"##LDAPFAULTYHOST##"
			/LDAPPort		##LDAPPORT##
			/LDAPTimeout		10
			/LDAPConnectionTimeout	5
			/LDAPBindName		"##BINDDN##"
			/LDAPBindPW		"##BINDPW##"
			/LDAPPooledConnections 0
			/LDAPRebindTimeout 0
			/LDAPTryAutoRebind		0
			/ConnectIsOk	0
			/ConnectRet 	"eBindNok"
			/Error {
				/Msg 		"Binding request failed. [Server: '##LDAPFAULTYHOST##', Port: '##LDAPPORT##']  (Authorized bind.)"
				/LdapCode	##BINDFAILCODE##
				/LdapMsg	"Can't connect to the LDAP server"
				/LdapConnectionParams {
					/Server "##LDAPFAULTYHOST##"
					/Port ##LDAPPORT##
					/Timeout 10
					/ConnectionTimeout 5
					/BindName "##BINDDN##"
					/BindPW "##BINDPW##"
					/PooledConnections 0
					/RebindTimeout 0
					/TryAutoRebind		0
					/MaxConnections 2
				}
				/MsgAsString ignore
			}
		}
		/WrongPort {
			/LDAPServer		"##LDAPHOST##"
			/LDAPPort		##LDAPFAULTYPORT##
			/LDAPTimeout		10
			/LDAPConnectionTimeout	5
			/LDAPBindName		"##BINDDN##"
			/LDAPBindPW		"##BINDPW##"
			/LDAPPooledConnections 0
			/LDAPRebindTimeout 0
			/LDAPTryAutoRebind		0
