			/LDAPConnectionTimeout 5
			/LDAPMaxConnections	   2
			/LDAPPooledConnections 1
			/LDAPTryAutoRebind			1
			/LDAPRebindTimeout 600

			/ConnectIsOk	1
			/ConnectRet 	"eOk"
		}
		/PooledConnectFollowUps {
			/NumberOfConnects 10
			/LDAPServer		"##LDAPHOST##"
			/LDAPPort		"##LDAPPORT##"
			/LDAPBind		"##BINDDN##"
			/LDAPBindPW		"##BINDPW##"
			/LDAPTimeout	10
			/LDAPConnectionTimeout 5
			/LDAPPooledConnections 1
			/LDAPMaxConnections	   2
			/LDAPTryAutoRebind			1
			/LDAPRebindTimeout 600

