	/CheckFaultyHeadersTest {
		/OnlyQuerySuccess {
			/RequestLine 	"GET ?bar HTTP/1.0\r\n\r\n"
			/Expected {
				/Reply	"HTTP/1.1 404 Not Found"
				/Request {
					/env {
						/REQUEST_URI "?bar"
					}
				}
			}
		}
		/OnlyRootURISuccess {
			/RequestLine 	"GET / HTTP/1.0\r\n\r\n"
			/Expected {
				/Request {
					/env {
						/REQUEST_URI "/"
					}
				}
				/Reply "HTTP/1.1 404 Not Found"
			}
		}
		/SimpleFileURISuccess {
				/RequestLine 	"GET /ftimage/image.gif HTTP/1.0\r\n\r\n"
				/Expected {
