	}
	/MethodTimeFormat {
	  "<"
	  { /Lookup TimeLogEntry.Key }
	  ">: "
	  { /Lookup TimeLogEntry.Msg }
	  "->"
	  { /Lookup TimeLogEntry.Time }
	  " "
	  { /Lookup TimeLogEntry.Unit }
	  " "
	  { /Lookup TimeLogEntry.NestingLevel }
	}
