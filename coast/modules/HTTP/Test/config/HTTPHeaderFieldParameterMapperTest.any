	  }
	  /ACCEPT {
		"*/*"
	  }
	  /USER-AGENT "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/535.2 (KHTML, like Gecko) Ubuntu/11.04 Chromium/15.0.865.0 Chrome/15.0.865.0 Safari/535.2"
