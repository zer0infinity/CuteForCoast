	/BoxColumnsSimpleTestWithPulldown {
		/List {
			{
				/Name	MC
				/Align	right
				/Width	4
				/Value	{
					/Lookup SelectBoxOption.marketCode
				}
				/Editable 0
				/Default	"J"
				/Pulldown {
					/ListData {
						{ /Text "*" }
						{ /Text "J" }
						{ /Text "N" }
						{ /Text "?" }
					}
					/TextRenderer {
						/Lookup	SelectBoxOption.Text
					}
					/ValueRenderer {
						/Lookup SelectBoxOption.Text
					}
					/SelectedRenderer {
						/StringCompareRenderer {
							/String1 {
								/Lookup {
									/Lookup {
										"fields.cbo"
										{ /Lookup Column.Name }
									}
								}
							}
							/String2	{ /Lookup SelectBoxOption.Text }
							/Equal		{ Column.Width }
							/Unequal	{ 0 }
						}
					}
					/Options			{
						/class		SelectBoxFooter
						/OnChange {
							"OnChangePulldown(this, "
							{ /Lookup ColumnIndex }
							")"
						}
					}
				}
			}
			{
				/Name	CC
				/Align	right
				/Width	4
				/Value	{
					/Lookup SelectBoxOption.currencyCode
				}
			}
			{
				/Name	Valor
				/Align	right
				/Width	8
				/Value	{
					/Lookup SelectBoxOption.valorNumber
				}
				/Editable 1
			}
			{
				/Name	Titelname
				/Align	left
				/Width	20
				/Value	{
					/Lookup SelectBoxOption.titleName
				}
			}
			{
				/Name	FIMS
				/Align	right
				/Width	4
				/Value	{
					/Lookup SelectBoxOption.fimsFlag
				}
			}
			{
				/Name	newest
				/Align	right
				/Width	6
				/Value	{
					/Lookup SelectBoxOption.newestFlag
				}
			}
		}
