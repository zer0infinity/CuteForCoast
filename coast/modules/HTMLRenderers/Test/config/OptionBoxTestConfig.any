D\n<script language=\"JavaScript1.2\" type=\"text/javascript\">\x0D\nfunction MyBox_DeleteButtonClicked(buttonObj)\x0D\n{\x0D\n\x09listbox=document.forms[0].elements['fld_MyBox'];\x0D\n\x09row=listbox.selectedIndex;\x0D\n\x09var bDoDelete = ((row>=0) && confirm('Wollen Sie die selektierte Zeile wirklich löschen?') );\x0D\n\x09if ( bDoDelete ) {\x0D\n\x09\x09MyBoxProperties.DeleteRow(row, listbox);\x0D\n\x09\x09document.forms[0].fld_edtMyBox_Added.value = MakeAnyFromFields(MyBoxProperties, MyBoxProperties.Changes.GetAddedFields());\x0D\n\x09\x09document.forms[0].fld_edtMyBox_Changed.value = MakeAnyFromFields(MyBoxProperties, MyBoxProperties.Changes.GetChangedFields());\x0D\n\x09\x09document.forms[0].fld_edtMyBox_Deleted.value = MakeAnyFromFields(MyBoxProperties, MyBoxProperties.Changes.GetDeletedFields());\x0D\n\x09\x09MyBox_ClearEditFields(document.forms[0]);\x0D\n\x09\x09listbox.selectedIndex = row;\x0D\n\x09};\x0D\n\x09return bDoDelete;\x0D\n};\x0D\n</script>\x0D\n<!-- hidden fields for editable box -->\n<INPUT TYPE=\"HIDDEN\" NAME=\"fld_edtMyBox_Added\">\n<INPUT TYPE=\"HIDDEN\" NAME=\"fld_edtMyBox_Changed\">\n<INPUT TYPE=\"HIDDEN\" NAME=\"fld_edtMyBox_Deleted\">\n\n<!-- Scripts for [MyBox] -->\n\n<script language=\"JavaScript1.2\" type=\"text/javascript\" src=\"ChangeableData.js\"></script>\n<script language=\"JavaScript1.2\" type=\"text/javascript\" src=\"OptionBoxScripts.js\"></script>\n\n<script language=\"JavaScript1.2\" type=\"text/javascript\">\n  <!-- hide from old browsers\n  function OnChangeMyBox(selbox)\n  {\n    var indx = selbox.selectedIndex;\n    var text = selbox[indx].value;\n    var myform = selbox.form;\n    Column = text.split(MyBoxProperties.ValueSep);\n    var bDoChange = true;\n    if (bDoChange)\n    {\n      document.forms[0].fld_edtMyBox_Column1.value = (Column[0] == undefined )?\"\":Column[0];\n      document.forms[0].fld_edtMyBox_Column2.value = (Column[1] == undefined )?\"\":Column[1];\n      document.forms[0].fld_edtMyBox_Column3.value = (Column[2] == undefined )?\"\":Column[2];\n      document.forms[0].fld_edtMyBox_Column4.value = (Column[3] == undefined )?\"\":Column[3];\n      document.forms[0].fld_edtMyBox_Column5.value = (Column[4] == undefined )?\"\":Column[4];\n    }\n    return true;\n  }\n\n  function MyBox_ClearEditFields(myform)\n  {\n    myform.fld_MyBox.selectedIndex = -1;\n    myform.fld_edtMyBox_Column1.value = \"\";\n    myform.fld_edtMyBox_Column2.value = \"\";\n    myform.fld_edtMyBox_Column3.value = \"\";\n    myform.fld_edtMyBox_Column4.value = \"\";\n    myform.fld_edtMyBox_Column5.value = \"\";\n    return false; /* do not submit the form*/\n  }\n  function MyBox_ProcessEditFieldScripts(myform)\n  {\n    return true;\n  }\n\n  function MyBox_OnChangeEditField(field, column, changeable, datatype)\n  {\n    listbox=field.form.elements[\"fld_MyBox\"];\n    for (var i = 0; i < listbox.length; i++) \n    {        if (listbox.options[i].selected == true)\n        { \n            row = listbox.options[i].index;\n            var bDoChange = true;\n            if (bDoChange == true && changeable == 0) \n            { \n    \x09          bDoChange = false; \n            } \n            if ((bDoChange == true && changeable == 1) && (row == -1 || listbox.options[row].value.length == 0)) \n            { \n    \x09          bDoChange = false; \n            } \n            if (bDoChange)\n            {\n                ChangeField( MyBoxProperties, row, column, field.value );\n                UpdateField( MyBoxProperties, row, column, field.value, listbox);\n            }\n            listbox.options[i].selected = true;\n        }\n    }\n    return true;\n  }\n\n  // create property variable for [MyBox]\n  var MyBoxProperties = new BoxProperties();\n  // now fill in the boxes data\n  MyBoxProperties.SetPrintTitle(\"\");\n  MyBoxProperties.SetValueSep(\";\");\n  MyBoxProperties.AddRow(\"\");\n  MyBoxProperties.AddRow(\"\");\n  MyBoxProperties.SetColumns(\"12;12;12;12;12\", \"center;right