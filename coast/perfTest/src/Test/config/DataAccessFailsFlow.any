{

	/NumberOfRuns	1		# Number of times the series is run
	/Run	{				# Inputconfiguration for the series of requests (run NumberOfRuns times)
		{	# initial address
			/CurrentServer {
				/ServerName "localhost"
				/Path		"/"
				/Port		"12345"
				/Timeout	1
			}
			/Token {
				{ /mustnot "a" }
				{ /mustnot "e" }
				{ /mustnot "i" }
			}
			/Input "GET / HTTP/1.0\n\n"
		}
	}

}
