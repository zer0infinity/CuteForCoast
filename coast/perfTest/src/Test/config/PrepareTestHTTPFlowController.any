{

	/NrOfRuns 1
	/Run {
		{
			/CurrentServer {
				/ServerName "Server"
				/Path "/Path/xy"
			}
			/Form {
				/slotNr		0
				/buttonPressed  "b_Ok"
				/formContents {
					/fld_Name "Hans Muster"
					/fld_Number "123"
					/fld_Checkbox "on"
				}
			}
		}
		{
			/Form {
				/slotNr		0
				/buttonPressed  "b_Ok"
				/formContents {
					/fld_Name "Hans Muster"
					/fld_Number "123"
				}
			}
		}
		{
			/Form {
				/slotNr		0
				/buttonPressed  "b_Ok"
				/formContents {
					/fld_Name "Hans Muster"
					/fld_Checkbox "_DoNotSend_"
				}
			}
		}
		{
			/Form {
				/slotNr		0
				/imageButtonPressed  "i_Ok"
				/formContents {
					/fld_Name "Hans Muster"
					/fld_Number "_DoNotSend_"
					/fld_Checkbox "_DoNotSend_"
				}
			}
		}
		{
			/CurrentServer {
				/ServerName "Server"
				/Path "/Path/xy"
				/Method "POST"
				/MsgBody	"Test"
			}
		}
		{
			/Link {
				/slotName "Link does not exist"
			}
		}
		{ # FormWithFileName
			/CurrentServer {
				/ServerName "Server"
				/Path "/Path/xy"
				/Method "POST"
			}
			/Form {
				/slotNr		0
				/buttonPressed  "b_Ok"
				/formContents {
					/fld_File {
						/Filename "hello.txt"
					}
				}
			}
		}
		{ # BoundarySeparatedMultipartForm
			/CurrentServer {
				/ServerName "Server"
				/Path "/Path/xy"
				/Method "POST"
				/Enctype "multipart/form-data"
			}
			/Form {
				/slotNr		0
				/buttonPressed  "b_Ok"
				/formContents {
					/fld_Name "Hans Muster"
					/fld_Note ""
				}
				/boundarySeparated "xxx" # Boundary specif. for multipart/form-data!
			}
		}
		{ # BoundarySeparatedMultipartFormWithFile
			/CurrentServer {
				/ServerName "Server"
				/Path "/Path/xy"
				/Method "POST"
				/Enctype "multipart/form-data"
			}
			/Form {
				/slotNr		0
				/buttonPressed  "b_Ok"
				/formContents {
					/fld_File {
						/Filename "hello.txt"
					}
					/fld_Note ""
				}
				/boundarySeparated "xxx" # Boundary specif. for multipart/form-data!
			}
		}
		{ # BoundarySeparatedMultipartFormWithFiles
			/CurrentServer {
				/ServerName "Server"
				/Path "/Path/xy"
				/Method "POST"
				/Enctype "multipart/form-data"
			}
			/Form {
				/slotNr		0
				/buttonPressed  "b_Ok"
				/formContents {
					/fld_File1 {
						/Filename "hello.txt"
					}
					/fld_File2 {
						/Filename "hello.txt" # If you'd like to checkin another file, please feel free to do so.'
					}
					/fld_Note ""
				}
				/boundarySeparated "xxx" # Boundary specif. for multipart/form-data!
			}
		}
	}
}
